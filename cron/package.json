{
  "name": "clash-stats-cron",
  "author": "Jason Rametta <rametta@outlook.com>",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "fluture": "^9.0.1",
    "numeral": "^2.0.6"
  },
  "scripts": {
    "dev-players": "nodemon --exec babel-node src/players.js",
    "dev-clan": "nodemon --exec babel-node src/clan.js",
    "dev-warlog": "nodemon --exec babel-node src/warlog.js",
    "clean": "rm -rf build && mkdir build",
    "build": "yarn clean && babel -d ./build ./src/players.js && babel -d ./build ./src/clan.js && babel -d ./build ./src/warlog.js"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
}
